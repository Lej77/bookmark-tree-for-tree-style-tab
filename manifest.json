{
  "manifest_version": 2,
  "name": "Bookmark Tree for Tree Style Tab",
  "version": "1.0",
  "default_locale": "en",
  "applications": {
    "gecko": {
      "strict_min_version": "57.0"
    }
  },
  "options_ui": {
    "page": "options/options.html"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "contextMenus",
    "notifications"
  ],
  "background": {
    "scripts": [
      "common.js",
      "background.js"
    ]
  }
}